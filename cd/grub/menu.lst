color = light-blue/blue black/green
timeout = 15
default = 1

title Memtest86+
find --set-root /udbcd
kernel /images/memtest

title FreeDOS Ultimate DOS Boot
find --set-root /udbcd
map /images/freedos.img (fd0)
map --hook
chainloader (fd0)+1
rootnoverify (fd0)

title MS-DOS 6.22
find --set-root /udbcd
map --mem /images/dos622.img (fd0)
map --hook
chainloader (fd0)+1
rootnoverify (fd0)

title MS-DOS 7.10
find --set-root /udbcd
map --mem /images/dos710.img (fd0)
map --hook
chainloader (fd0)+1
rootnoverify (fd0)

title MaxDOS
find --set-root /udbcd
map --mem /images/maxdos.img (fd0)
map --hook
chainloader (fd0)+1
rootnoverify (fd0)

title Reboot
find --set-root /udbcd
reboot

title <-- Return
find --set-root /udbcd
quit

title Poweroff
find --set-root /udbcd
poweroff

title Damn Small Linux 4.4.10 [128MB RAM]
find --set-root /udbcd
kernel /images/dsllin24 ramdisk_size=58440 init=/etc/init lang=us apm=power-off vga=789 nomce noapic BOOT_IMAGE=dsl
initrd /images/dslrt24.gz

